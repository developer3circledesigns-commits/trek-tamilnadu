{
    "version": 2,
    "builds": [
        {
            "src": "*.php",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "."
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.php"
        }
    ]
}
